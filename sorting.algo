ALGORITHM algorithm_name
VAR
    array : ARRAY_OF INTEGER[10]; 
    i,j,t:int; 
BEGIN
    FUNCTION insertionsort(array) {
        for ( i=0;i<array.length-1;i++){
                t =array[i];
            for( j=i-1;j>-1 && array[i]>t;j--){
                array[j+1]=array[j] ;   }
                array[j+1]=t;
        }
        return array;
    }
    
END















